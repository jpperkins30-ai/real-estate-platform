{
  "name": "real-estate-platform-server",
  "version": "1.0.0",
  "description": "Server for the real estate platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --project tsconfig.json src/index.ts",
    "start:with-env": "cross-env-shell \"node -r dotenv/config start-env-helper.js && nodemon --exec ts-node -r tsconfig-paths/register --project tsconfig.json src/index.ts\"",
    "start:js": "node src/index.js",
    "dev:js": "nodemon src/index.js",
    "simple": "node src/simple-server.js",
    "build": "tsc",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "flatten-tests": "powershell -File ./scripts/flatten-test-structure.ps1",
    "rename-test-dirs": "powershell -File ./scripts/rename-test-directories.ps1",
    "standardize-test-dir": "powershell -File ./scripts/standardize-test-dir.ps1",
    "precommit": "cd .. && powershell -File ./scripts/universal-pre-commit-check.ps1 -ProjectType server"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.6",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^4.17.17",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.15.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/xlsx": "^0.0.36",
    "@vitest/coverage-v8": "^1.3.0",
    "jsdom": "^26.0.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^3.1.9",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vitest": "^1.3.0"
  }
}
